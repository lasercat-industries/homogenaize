{
  "name": "homogenaize",
  "version": "0.1.0",
  "description": "A TypeScript-native library that provides a unified interface for multiple LLM providers (OpenAI, Anthropic, Gemini)",
  "author": "Ryland Goldstein",
  "license": "MIT",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "type": "module",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "keywords": [
    "llm",
    "ai",
    "openai",
    "anthropic",
    "gemini",
    "typescript",
    "unified-api",
    "gpt",
    "claude",
    "structured-output",
    "zod"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/rylandgoldstein/homogenaize.git"
  },
  "bugs": {
    "url": "https://github.com/rylandgoldstein/homogenaize/issues"
  },
  "homepage": "https://github.com/rylandgoldstein/homogenaize#readme",
  "scripts": {
    "build": "bun build ./index.ts --outdir ./dist --format esm --sourcemap && tsc --emitDeclarationOnly --declaration --outDir dist",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "format": "echo 'Format script not configured yet'",
    "prepublishOnly": "bun run build && bun run test && bun run typecheck"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@types/bun": "latest",
    "@types/node": "^24.0.13",
    "@vitest/coverage-v8": "^3.2.4",
    "dotenv": "^17.2.0",
    "eslint": "^9.30.1",
    "happy-dom": "^18.0.1",
    "typescript-eslint": "^8.36.0",
    "vite": "^7.0.4",
    "vitest": "^3.2.4"
  },
  "peerDependencies": {
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "zod": "^4.0.0",
    "zod-to-json-schema": "^3.24.6"
  }
}
